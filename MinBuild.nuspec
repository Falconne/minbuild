<?xml version="1.0"?>
<package>
  <metadata>
    <id>MinBuild</id>
    <version>$version$</version>
    <authors>Anuradha Dissanayake</authors>
    <owners>Anuradha Dissanayake</owners>
    <projectUrl>https://github.com/Falconne/minbuild</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>msbuild task to cache build outputs of C# and C++ projects, for reuse when when the same source is rebuilt</description>
  </metadata>
  <files>
    <file src="CacheArtifacts\bin\Release\*.exe" target="tools\CacheArtifacts" />
    <file src="CacheArtifacts\bin\Release\*.dll" target="tools\CacheArtifacts" />
    <file src="CacheArtifacts\bin\Release\*.config" target="tools\CacheArtifacts" />
    <file src="CacheArtifacts\bin\Release\*.targets" target="tools\CacheArtifacts" />
    <file src="CacheArtifacts\bin\Release\*.build" target="tools\CacheArtifacts" />
    <file src="CacheCleanup\bin\Release\*.exe" target="tools\CacheCleanup" />
    <file src="CacheCleanup\bin\Release\*.dll" target="tools\CacheCleanup" />
    <file src="CacheCleanup\bin\Release\*.config" target="tools\CacheCleanup" />
  </files>
</package>
